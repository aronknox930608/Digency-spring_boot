<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet author="felkasmi" id="CREATE_TABLE_DEMAND_PRICE" logicalFilePath="acm"
               runOnChange="true">
        <createTable tableName="DEMAND_PRICE">
            <column autoIncrement="true" name="ID" type="BIGINT">
                <constraints primaryKey="true" primaryKeyName="PK_DEMAND_PRICE"/>
            </column>
            <column name="REF_DEMAND" type="VARCHAR(50)"/>
            <column name="STATUS" type="VARCHAR(255)"/>
            <column name="DECISION_DATE" type="DATE"/>
            <column name="COMMENT" type="VARCHAR(500)"/>
            <column name="KEY" type="VARCHAR(150)"/>
            <column name="REF_ARTIST_ACCOUNT" type="VARCHAR(50)"/>
            <column name="REF_AWARD" type="VARCHAR(50)"/>
            <column name="REF_AWARD_CATEGORY" type="VARCHAR(50)"/>
            <column name="REF_THEATER_PIECE" type="VARCHAR(50)"/>
            <column name="REF_BOOK" type="VARCHAR(20)"/>
            <column name="DATE_CREATION" type="TIMESTAMP"/>
            <column name="DATE_MODIFICATION" type="TIMESTAMP"/>
            <column name="MODIFICATION_PAR" type="VARCHAR(255)"/>
            <column name="CREATION_PAR" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>



